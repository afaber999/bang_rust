var i: i64;
var heap_base : ptr;
var count: i64;

proc main() {
  count = 6;
  i = 0;
  while i < count {
    store_ptr( 
        u8, 
        heap_base + cast(ptr, i), 
        cast(u8, ( ( (2*i) + 65) + 1 )+10) );

    i = i + 1;
  }

  store_ptr( 
      u8, 
      heap_base + cast(ptr, count), 
      cast(u8, 10) );

  write_ptr( heap_base, count + 1 );
}
